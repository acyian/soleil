{% extends "::SousSite/Admin/baseAdmin.html.twig" %}

{% block content %}

    <div class="row">
        
          <div class="col-lg-12">
            <h1>Réalisation - voir</h1>
            <ol class="breadcrumb">
              <li><a href="index.html"><i class="fa fa-dashboard"></i> Accueil</a></li>
              <li class="active"><i class="fa fa-table"></i> Réalisation</li>
            </ol>
          </div>
        
  </div><!-- /.row -->
    
  Titre : {{ realisation.titreFr }} <br/>
  Realisé par : {{ realisation.realiserpar }} <br/>
  Prix : {{ realisation.prix }} <br/> 
  Dimensions : {{ realisation.dimension }} <br/>
  
  <figure>
							
  <img
  src="{{ asset(realisation.image.webPath) }}"
  alt="{{ realisation.titreFr}}" class="taillePetitImage"
  />
                                                        
  </figure>
{% endblock %}